{
    "name": "bulma-responsive-images",
    "version": "1.0.0",
    "description": "demo",
    "license": "ISC",
    "author": "rasx@songhaysytem.com",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "build": "npm run sass:bundle && npm run webpack:bundle",
        "start": "npx webpack serve --client-overlay --name=optimization-config --mode=production --static-directory=./",
        "start:development": "npx webpack serve --client-overlay --name=default-config --mode=development --static-directory=./",
        "sass:bundle": "npx sass ./src/scss/index.scss ./_bundles/styles.min.css --load-path=./node_modules --no-source-map --style=compressed",
        "webpack:bundle": "npx webpack --config-name=optimization-config --mode=production"
    },
    "dependencies": {
        "bulma": "^1.0.4",
        "songhay": "^0.3.7"
    },
    "devDependencies": {
        "sass": "^1.97.1",
        "terser-webpack-plugin": "^5.3.16",
        "ts-loader": "^9.5.4",
        "tslib": "^2.8.1",
        "typescript": "^5.9.3",
        "webpack": "^5.104.1",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2"
    }
}
