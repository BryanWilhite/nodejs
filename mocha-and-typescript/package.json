{
    "name": "mocha-and-typescript",
    "version": "1.0.0",
    "description": "see README",
    "scripts": {
        "lerna:build:typescript": "npx lerna exec -- tsc",
        "lerna:clean": "npx lerna clean --yes",
        "lerna:install": "npx lerna exec -- npm i",
        "ts-mocha:test:mocha-getting-started": "npx ts-mocha -p packages/mocha-getting-started/__tests__/tsconfig.json packages/mocha-getting-started/__tests__/**/*.spec.ts",
        "ts-mocha:test:mocha-getting-started-browser-1": "http-server packages/mocha-getting-started-browser/__tests__/step-1 -o -c-1",
        "ts-mocha:test:mocha-getting-started-browser-2": "tsc --project packages/mocha-getting-started-browser/__tests__/step-2/tsconfig.json && http-server packages/mocha-getting-started-browser/__tests__/step-2 -o -c-1",
        "ts-mocha:test:mocha-getting-started-browser-3": "tsc --project packages/mocha-getting-started-browser/__tests__/step-3/tsconfig.json && http-server . -o -c-1",
        "ts-mocha:test:mocha-getting-started-browser-4": "tsc --project packages/mocha-getting-started-browser/__tests__/step-4/tsconfig.json && http-server . -o -c-1",
        "ts-mocha:test:mocha-getting-started-browser-5": "tsc --project packages/mocha-getting-started-browser/__tests__/step-5/tsconfig.json && http-server . -o -c-1"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@material/typography": "6.0.0",
        "@material/layout-grid": "6.0.0",
        "normalize.css": "8.0.1",
        "tslib": "1.12.0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.11",
        "@types/expect": "^24.3.0",
        "@types/mocha": "^7.0.2",
        "chai": "^4.2.0",
        "lerna": "^3.20.2",
        "mocha": "^7.1.2",
        "ts-mocha": "^7.0.0",
        "tslint": "^6.1.2",
        "typescript": "^3.8.3"
    }
}
