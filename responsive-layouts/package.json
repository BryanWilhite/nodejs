{
  "name": "songhay-index-layout",
  "version": "1.0.0",
  "description": "responsive layouts",
  "scripts": {
    "blog-index:sass:transpile": "npx sass ./blog-index/src/scss/index.scss ./blog-index/src/css/index.css --load-path ./node_modules",
    "index:sass:transpile": "npx sass ./index/src/scss/index.scss ./index/src/css/index.css --load-path ./node_modules",
    "start:blog-index": "npx webpack serve --mode=development --config-name=blog-index-config",
    "start:index": "npx webpack serve --mode=development --config-name=index-config",
    "webpack:bundle": "npx webpack --mode=production"
  },
  "author": "rasx@songhaysytem.com",
  "license": "ISC",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "sass": "^1.29.0",
    "terser-webpack-plugin": "^5.0.3",
    "ts-loader": "^8.0.10",
    "tslint": "^6.1.3",
    "typescript": "^4.0.5",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@material/top-app-bar": "^8.0.0",
    "@material/typography": "^8.0.0",
    "lit-html": "^1.3.0",
    "normalize.css": "^8.0.1",
    "rxjs": "^6.6.3"
  }
}
